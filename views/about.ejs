<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <title>Game</title>
    <link rel="stylesheet" href="/styles/main.css" type="text/css" />
    <script type="text/javascript" src="/vendor/jquery-3.0.0.js"></script>
    <script src="https://cdn.jsdelivr.net/handlebarsjs/4.0.5/handlebars.min.js" integrity="sha256-rMOSOM5HDzVEMoVZTv2189+RKSTSgY5ZKfTfap7q2zE=" crossorigin="anonymous"></script>
    <script type="text/javascript" src="/scripts/app.js"></script>
  </head>
  <body>
    <section id='hand-container'>

    </section>
    <script id="hand-template" type="text/x-handlebars-template">
      <section class='userHand'>
              {{#each userhand}}
                  <span class="card-front" data-rank-idx="{{this.[0]}}" data-suit-idx="{{this.[1]}}"> </span>
              {{/each}}
      </section>
    </script>
    <section class="card-back" class="shuffling-cards">
      <div id="all">
        <div id="card1" class="shuffle"></div>
        <div id="card2" class="shuffle"></div>
        <div id="text" class="text">
          <p>Card shuffling</p>
        </div>
      </div>
    </section>
  </body>
</html>
